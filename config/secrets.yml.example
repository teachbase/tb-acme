settings: &default
  deploy_host: <%= ENV['DEPLOY_HOST'] %>
  remote_path: <%= ENV['REMOTE_PATH'] %>
  username: <%= ENV['USERNAME'] %>
  repository: https://github.com/teachbase/tb-acme
  acme_endpoint: <%= ENV['ACME_ENDPOINT'] %>
  public_path: <%= ENV['PUBLIC_PATH'] %>
  private_path: <%= ENV['PRIVATE_PATH'] %>
  owner_email: 'mailto:help@teachbase.ru'
  front_domain: <%= ENV['FRONT_DOMAIN'] %>
  raven_dsn: <%= ENV['SENTRY_DSN'] %>
  cloud_redis: <%= ENV['CLOUD_REDIS'] %>
  acme_token_bucket: <%= ENV['ACME_TOKEN_BUCKET'] %>
  token_storage: <%= ENV['ACME_TOKEN_STORAGE'] %>
  redis_master:
    url: <%= ENV['REDIS_MASTER_URL'] %>
  s3:
    region: <%= ENV['S3_REGION'] %>
    endpoint: <%= ENV['S3_ENDPOINT'] %>
    access_key_id: <%= ENV['S3_ACCESS_KEY'] %>
    secret_access_key: <%= ENV['S3_SECRET_KEY'] %>

development:
  <<: *default

test:
  <<: *default

staging:
  <<: *default

production:
  <<: *default
